<?xml version="1.0" ?>

<!-- File: config/services.xml -->

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="rokkacli.provider.client" class="RokkaCli\Provider\ClientProvider">
            <argument type="service" id="rokkacli.configuration"/>
        </service>

        <service id="rokkacli.helper.rokka" class="RokkaCli\RokkaApiHelper">
            <argument type="service" id="rokkacli.configuration"/>
        </service>

    </services>
</container>
